# Proxy Groups
proxy-groups:
  - name: "ProxyMama-Selector"
    type: select
    proxies:
      - "ProxyMama-Load-Balance"
      - "ProxyMama100 Server-1"
      - "ProxyMama100 Server-2"
      - "ProxyMama100 Server-3"
      - "ProxyMama100 Server-4"
      - "ProxyMama100 Server-5"
      - "ProxyMama100 Server-6"
      - "ProxyMama100 Server-7"
      - "ProxyMama100 Server-8"
      - "ProxyMama100 Server-9"
      - "ProxyMama100 Server-10"
      - DIRECT

  - name: "ProxyMama-Load-Balance"
    type: load-balance
    strategy: consistent-hashing
    url: 'http://www.gstatic.com/generate_204'
    interval: 300
    proxies:
      - "ProxyMama100 Server-1"
      - "ProxyMama100 Server-2"
      - "ProxyMama100 Server-3"
      - "ProxyMama100 Server-4"
      - "ProxyMama100 Server-5"
      - "ProxyMama100 Server-6"
      - "ProxyMama100 Server-7"
      - "ProxyMama100 Server-8"
      - "ProxyMama100 Server-9"
      - "ProxyMama100 Server-10"

# Rules
rules:
  - DOMAIN-SUFFIX,google.com,ProxyMama-Selector
  - DOMAIN-SUFFIX,github.com,ProxyMama-Selector
  - DOMAIN-KEYWORD,bdixtv,DIRECT
  - DOMAIN-KEYWORD,bdix,DIRECT
  - GEOIP,BD,DIRECT
  - MATCH,ProxyMama-Selector
